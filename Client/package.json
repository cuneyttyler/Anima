{
  "name": "anima",
  "scripts": {
    "start": "ts-node Anima.ts",
    "build-web": "cd webapp/vue/ && npm run build",
    "build-app": "npx tsc && npx caxa --input . --exclude \".env\" \"*.log\" \"package-lock.json\" \"World\" \".git\" \"*.exe\" --prepare-command \"tsc\" \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/jsbuild/Anima.js\" --output Anima.exe",
    "build": "npm run build-web && npm run build-app"
  },
  "extends": "./tsconfig.json",
  "dependencies": {
    "@fastify/websocket": "^7.1.2",
    "@google-cloud/text-to-speech": "^5.3.0",
    "@google-cloud/vertexai": "^1.3.0",
    "@grpc/grpc-js": "^1.10.9",
    "@stackpath/node-grpc-error-details": "*",
    "audiobuffer-to-wav": "^1.0.0",
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "dependencies": "^0.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "fastify": "^4.12.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "google-gax": "^4.3.8",
    "music-metadata": "^8.3.0",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^4.52.4",
    "sync-exec": "^0.6.2",
    "sync-rest-client": "^1.0.3",
    "typescript": "^5.5.3",
    "uuid": "^10.0.0",
    "vue": "^2.7.16",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "ts-node": "^10.9.2"
  },
  "type": "module"
}
